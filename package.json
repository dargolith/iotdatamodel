{
  "name": "iotdatamodel",
  "type": "module",
  "version": "5.0.1",
  "description": "Generic data model for Internet of Things",
  "main": "dist/index.cjs",
  "engines": {
    "node": ">=14.17.0"
  },
  "source": "index.mjs",
  "exports": "dist/index.mjs",
  "INACTIVE_exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "module": "dist/index.module.mjs",
