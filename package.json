  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dargolith/iotdatamodel/issues"
  },
  "homepage": "https://github.com/dargolith/iotdatamodel#readme",
  "devDependencies": {
    "microbundle": "^0.15.0",
    "replace": "^1.2.1"
  }
